{
  "name": "gatsby-remark-plantuml-lite",
  "version": "0.1.3",
  "description": "A Gatsby plugin to transform PlantUML code blocks into SVG images.",
  "main": "dist/index.js",
  "types": "dist/*.d.ts",
  "homepage": "https://github.com/Mogeko/gatsby-remark-plantuml-lite",
  "repository": "git@github.com:Mogeko/gatsby-remark-plantuml-lite.git",
  "bugs": {
    "url": "https://github.com/Mogeko/gatsby-remark-plantuml-lite/issues",
    "email": "zhengjunyi@live.com"
  },
  "author": "Mogeko <zhengjunyi@live.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "remark",
    "plantuml"
  ],
  "files": [
    "dist/*",
    "gatsby-node.js"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist coverage",
    "release": "yarn build && yarn npm publish",
    "lint": "prettier --check .",
    "fmt": "prettier --write .",
    "test": "jest ./tests",
    "cov": "jest --coverage"
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@babel/runtime": "^7.24.7",
    "@types/jest": "^27.5.2",
    "@types/mdast": "^3.0.15",
    "@types/node": "^17.0.45",
    "gatsby-plugin-utils": "^2.5.0",
    "jest": "^27.5.1",
    "mdast-util-from-markdown": "0.8.5",
    "prettier": "^2.8.8",
    "ts-jest": "^27.1.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "plantuml-encoder": "^1.4.0",
    "unist-util-flatmap": "^1.0.0"
  },
  "packageManager": "yarn@4.3.0",
  "engines": {
    "node": ">=18.20.3",
    "npm": ">=10.7.8"
  }
}
